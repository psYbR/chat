  "scripts": {
    "serve": "node_modules/.bin/webpack-dev-server",
    "webpack-build": "node_modules/.bin/webpack",
    "webpack-prod": "node_modules/.bin/webpack -p --config webpack.config.prod.js",
    "webpack-watch": "node_modules/.bin/webpack --watch",
    "copymap": "node_modules/.bin/cpx \"public/bundle.js.map\" \"server/public\"",
    "delmap": "node_modules/.bin/del-cli server/public/bundle.js.map",
    "copybundle": "node_modules/.bin/cpx \"public/bundle.js\" \"server/public\"",
    "copybundle-prod": "node_modules/.bin/cpx \"public/bundle.js\" \"server/public\"",
    "copybundle-watch": "node_modules/.bin/cpx \"public/bundle.js\" \"server/public\" --watch",
    "build": "npm run webpack-build && npm run copybundle && npm run copymap",
    "build-prod": "npm run webpack-prod && npm run copybundle-prod && npm run delmap",
    "watch": "node_modules/.bin/concurrently \"npm run copybundle-watch\" \"npm run webpack-watch\""
  }
